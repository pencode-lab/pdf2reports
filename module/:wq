class Sheet:
    '''
    class for ..
    '''
    
    def __init__(self,xml_root,name,start_key,stop_key):

        if not xml_root:
            raise ValueError("parameter 'xml_root' must be a non-empty, non-None")

        self.name = name
        self.start_key =  start_keyy
        self.stop_key = stop_key

        self.findStartKey = False
        self.needStop = False

        self.sheetRows.clear()
        self.aRow.clear()
        self.tmpValue=''



